{
  "name": "@gmod/bgzf-filehandle",
  "version": "1.4.7",
  "description": "read from a compressed bgzip file (with .gzi) as if it were uncompressed",
  "license": "MIT",
  "repository": "gmod/bgzf-filehandle",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "test": "vitest",
    "lint": "eslint src test",
    "clean": "rimraf dist esm",
    "prebuild": "yarn clean",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:es5": "tsc --target es5 --outDir dist",
    "build": "yarn build:esm && yarn build:es5",
    "prepublishOnly": "yarn test --run && yarn build",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "bionode",
    "biojs"
  ],
  "dependencies": {
    "es6-promisify": "^7.0.0",
    "generic-filehandle": "^3.0.0",
    "long": "^4.0.0",
    "pako": "^1.0.11"
  },
  "devDependencies": {
    "@types/es6-promisify": "^6.0.0",
    "@types/long": "^4.0.1",
    "@types/node": "^18.11.16",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "./esm/unzip.js": "./esm/unzip-pako.js"
  }
}
