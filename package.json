{
  "name": "@gmod/bgzf-filehandle",
  "version": "1.4.0",
  "description": "read from a compressed bgzip file (with .gzi) as if it were uncompressed",
  "license": "MIT",
  "repository": "gmod/bgzf-filehandle",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist esm",
    "prebuild": "npm run clean",
    "build": "tsc --target es2018 --outDir esm && tsc --target es5 --outDir dist",
    "prepublishOnly": "npm test && npm run build",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "bionode",
    "biojs"
  ],
  "dependencies": {
    "es6-promisify": "^7.0.0",
    "generic-filehandle": "^2.2.1",
    "long": "^5.1.0",
    "pako": "^1.0.11"
  },
  "devDependencies": {
    "@types/es6-promisify": "^6.0.0",
    "@types/jest": "^27.0.3",
    "@types/long": "^4.0.1",
    "@types/node": "^16.11.10",
    "@types/pako": "^1.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "./esm/unzip.js": "./esm/unzip-pako.js"
  }
}
