{
  "name": "@gmod/bgzf-filehandle",
  "version": "1.4.5",
  "description": "read from a compressed bgzip file (with .gzi) as if it were uncompressed",
  "license": "MIT",
  "repository": "gmod/bgzf-filehandle",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src test",
    "clean": "rimraf dist esm",
    "prebuild": "npm run clean",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:es5": "tsc --target es5 --outDir dist",
    "build": "npm run build:esm && npm run build:es5",
    "prepublishOnly": "npm test && npm run build",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "bionode",
    "biojs"
  ],
  "dependencies": {
    "es6-promisify": "^7.0.0",
    "generic-filehandle": "^3.0.0",
    "long": "^5.1.0",
    "pako": "^1.0.11"
  },
  "devDependencies": {
    "@types/es6-promisify": "^6.0.0",
    "@types/jest": "^27.0.3",
    "@types/long": "^4.0.1",
    "@types/node": "^16.11.10",
    "@types/pako": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "./esm/unzip.js": "./esm/unzip-pako.js"
  }
}
